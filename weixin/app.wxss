@import 'static/style/weui.wxss';

.page {
    background-color: #f0f0f0;
}

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
} 

.float-cart {
    position: fixed;
    bottom: 20px;
    right: 20px;
    width: 60px;
    height: 60px;
}

.float-cart image {
    width: 50px;
    height: 50px;
    margin: 5px;
}

.mini-btn{
    margin-right: 5px;
}
